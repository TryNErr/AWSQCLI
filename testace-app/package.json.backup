{
  "name": "testace-lambda-backend",
  "version": "1.0.0",
  "description": "TestAce Backend for AWS Lambda",
  "main": "lambda-handler.js",
  "scripts": {
    "start": "serverless offline",
    "deploy": "serverless deploy",
    "deploy:prod": "serverless deploy --stage prod",
    "deploy:dev": "serverless deploy --stage dev",
    "remove": "serverless remove",
    "logs": "serverless logs -f api",
    "test": "node test-deployment.js",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "start:frontend": "cd frontend && npm start",
    "start:backend": "cd backend && npm run dev",
    "build:all": "cd frontend && npm run build && cd ../backend && npm run build",
    "clean": "rm -rf node_modules frontend/node_modules backend/node_modules",
    "update:deps": "npm update && cd frontend && npm update && cd ../backend && npm update"
  },
  "dependencies": {
    "express": "^4.21.2",
    "serverless-http": "^3.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "serverless": "^4.4.36",
    "serverless-offline": "^14.4.0",
    "axios": "^1.7.9"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "testace",
    "lambda",
    "serverless",
    "aws",
    "education"
  ],
  "author": "TestAce Team",
  "license": "MIT",
  "overrides": {
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@^1.5.0",
    "@jridgewell/sourcemap-codec": "^1.5.0"
  },
  "resolutions": {
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@^1.5.0",
    "@jridgewell/sourcemap-codec": "^1.5.0"
  }
}
